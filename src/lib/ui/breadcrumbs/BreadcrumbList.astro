---
interface Props {
  class?: string;
  'data-testid'?: string;
}

const { class: className, 'data-testid': dataTestId } = Astro.props;
---

<nav class={`max-w-4xl mx-auto mt-4 text-sm text-gray-600 ${className}`} data-testid={dataTestId}>
  <ul class="flex space-x-2">
    <slot />
  </ul>
</nav>
